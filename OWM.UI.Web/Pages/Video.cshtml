@page "/Content/Video"
@model OWM.UI.Web.Pages.Video
@{
    ViewData["Title"] = "Videos";
    Layout = "Shared/_Layout";
}
<style>
    .nopadding {
        padding: 2px !important;
        margin: 0 !important;
    }
</style>
<div class="container text-center">
    <div class="card">
        <div class="card-header text-left">
           Videos
        </div>
        <div class="card-body">
            <div class="row pl-4 pr-4 pt-2 pb-2" id="videos-contents">
                <script>
                    $(function () {
                        $('#videos-contents').load('/videos_page/videos.html');
                    })
                </script>
            </div>
        </div>
    </div>
</div>
